<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
	
<mapper namespace="mapper.usePoint">
	<insert id="insertUsePoint" parameterType="usePoint" useGeneratedKeys="true" keyProperty="upNum">
		insert into usepoint (userNum,usedPoint,type,currPoint) VALUE(#{userNum},#{usedPoint},#{type},#{currPoint});
	</insert>
</mapper>